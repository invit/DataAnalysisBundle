<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="invit.admin.data_analysis_category" class="Invit\DataAnalysisBundle\Admin\DataAnalysisCategoryAdmin">
            <tag name="sonata.admin" manager_type="orm" group="weekend4two" label="Kategorien"/>
            <argument />
            <argument>Invit\DataAnalysisBundle\Entity\DataAnalysisCategory</argument>
            <argument>SonataAdminBundle:CRUD</argument>
        </service>
        <service id="invit.admin.data_analysis_query_parameter" class="Invit\DataAnalysisBundle\Admin\DataAnalysisQueryParameterAdmin">
            <tag name="sonata.admin" manager_type="orm" group="weekend4two" label="Parameter"/>
            <argument />
            <argument>Invit\DataAnalysisBundle\Entity\DataAnalysisQueryParameter</argument>
            <argument>SonataAdminBundle:CRUD</argument>
        </service>
        <service id="invit.admin.data_analysis_query" class="Invit\DataAnalysisBundle\Admin\DataAnalysisQueryAdmin">
            <tag name="sonata.admin" manager_type="orm" group="weekend4two" label="Auswertungen"/>
            <argument />
            <argument>Invit\DataAnalysisBundle\Entity\DataAnalysisQuery</argument>
            <argument>InvitDataAnalysisBundle:DataAnalysisQueryAdmin</argument>
            <call method="addChild">
                <argument type="service" id="invit.admin.data_analysis_query_subscription" />
            </call>
        </service>
        <service id="invit.admin.data_analysis_query_subscription" class="Invit\DataAnalysisBundle\Admin\DataAnalysisQuerySubscriptionAdmin">
            <tag name="sonata.admin" manager_type="orm" group="weekend4two" label="Auswertungen"/>
            <argument />
            <argument>Invit\DataAnalysisBundle\Entity\DataAnalysisQuerySubscription</argument>
            <argument>SonataAdminBundle:CRUD</argument>
        </service>
    </services>
</container>
